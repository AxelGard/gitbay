<script type="text/javascript">
function delay(ms) {
    return new Promise(resolve => setTimeout(resolve, ms));
}
function windowIsLoaded() {
    return document.readyState === "complete";// || document.readyState === "interactive";
}

async function windowLoaded() {
    return new Promise(function (resolve) {
        if (windowIsLoaded()) {
           //document.getElementsByClassName('cs-icon-search')[1].style.color = 'yellow';
            resolve();
        } else {
           let intervalTest = setInterval(function(){
                     if(windowIsLoaded()){
                        //document.getElementsByClassName('cs-icon-search')[1].style.color = 'green';
                         clearInterval(intervalTest)
                         resolve();
                     }
           }, 150);
            window.onload = (event) => {
                //document.getElementsByClassName('cs-icon-search')[1].style.color = 'blue';
                resolve();
            };
           window.addEventListener('load', function () {
                 //document.getElementsByClassName('cs-icon-search')[1].style.color = 'purple';
                resolve();
           })
        }
    });
}



async function lazyLoadScript(scriptUrl, id, ele){
const oldScript = document.querySelector(`#${id}`);
if(oldScript) return;

const s = document.createElement("script");
s.id = id;
s.type = "text/javascript";
s.language="language";
s.src = scriptUrl;
s.setAttribute("async","");
s.setAttribute("defer","");
if(!ele) ele = document.body;
ele.appendChild(s);

const script = document.querySelector(`#${id}`);
return new Promise(function(resolve){
script.addEventListener('load', function() {
    resolve();
  });
})
}

async function loadPubliftAd(id){
//return;
  try{
   await windowLoaded();
   await delay(50);
   await lazyLoadScript('https://cdn.fuseplatform.net/publift/tags/2/2511/fuse.js', 'publiftScript');
   fusetag.loadSlotById(id);
   console.log('LOADED PUBLIFT AD!!!!!!!!!!!!!');
  }catch(e){console.log('ERROR LOADING PUBLIFT AD:::::::::::::', e)}
}

async function loadPrimis(element){
//return;
console.log('Primis waiting for window load')
await windowLoaded();
console.log('Primis window loaded')
await delay(50);
await lazyLoadScript('https://live.primis.tech/live/liveView.php?s=109058', 'primisScript', element);
console.log('LOADED PRIMIS!!!!!!!!!');
}

</script>
