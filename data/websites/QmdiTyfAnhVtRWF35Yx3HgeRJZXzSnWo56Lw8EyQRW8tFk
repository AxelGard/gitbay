							let time_ago = timestamp - data_time;
							if (time_ago < 60) {
								time_ago = Math.round(time_ago) + ' seconds ago';
							} else if (time_ago < 3600) {
								time_ago = Math.round(time_ago / 60) + ' minutes ago';
							} else if (time_ago < 86400) {
								time_ago = Math.round((time_ago / 60) / 60) + ' hours ago';
							} else if (time_ago < 604800) {
								time_ago = Math.round(((time_ago / 60) / 60) / 24) + ' days ago';
